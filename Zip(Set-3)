  CREATE TABLE boats(
  bid INT NOT NULL,
  bname CHAR(40) NULL,
  color CHAR(20) NULL,
  PRIMARY KEY (bid));
  
INSERT INTO boats (bid, bname, color) VALUES ('101', 'A', 'red');
