SELECT s.sname, s.age
FROM Sailors AS s
WHERE S.rating > 7;

SELECT s.sname, r.bid, r.day 
FROM Sailors s, Reserves r
WHERE s.sid = r.sid; 

SELECT s.sname
FROM Sailors s
WHERE s.sid IN(SELECT r.sid
FROM Reserves r 
WHERE r.day = '2022-01-01');
